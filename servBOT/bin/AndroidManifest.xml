<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.servbot"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.servbot.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.servbot.RestaurantList"
            android:label="@string/title_activity_restaurant_list" >
        </activity>
        <activity
            android:name="com.example.servbot.RestaurantMenu"
            android:label="@string/title_activity_restaurant_menu" >
        </activity>
        <activity
            android:name="com.example.servbot.Order"
            android:label="@string/title_activity_order" >
        </activity>
    </application>

</manifest>
